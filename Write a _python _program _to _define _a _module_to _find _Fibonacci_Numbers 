def generate_fibonacci_sequence(n: int):
    if n < 0:
        print("Please enter a positive integer")
    else:
        print(f"Fibonacci series up to {n}:")
        a, b = 0, 1
        for _ in range(n):
            print(f"{a} ", end="")
            a, b = b, a + b

# Main program
n = int(input("Enter the number of terms: "))
generate_fibonacci_sequence(n)